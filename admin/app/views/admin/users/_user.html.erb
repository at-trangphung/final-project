<tr>
  <td><%= user.id %></td>
  <td><%= user.id%></td>
  <%# if user.avatar? %>
    <%#= image_tag user.avatar.url, :size => "70x50" %>
  <%# end %>
  <td><%= user.first_name%></td>
  <td><%= user.last_name%></td>
  <td><%= user.email %></td>
  <td><%= user.address %></td>
  <td><%= user.phone %></td>
  <td>
    <%= link_to user_path(user), method: :delete,
      data: {confirm: "Do you want this user"} do %>
      <i class="pe-7s-trash"></i>
    <% end %>
  </td>
</tr>
