<div class="panel panel-default">
  <div class="panel-heading">
    <%= "List order" %>
  </div>
  <div class="panel-body">
    <div class="wishlist-item table-responsive">
      <table class="table">
        <thead>
          <tr>
            <th>id</th>
            <th>user email</th>
            <th>first name</th>
            <th>last name</th>
            <th>total</th>
            <th>status</th>
            <th>function</th>
          </tr>
        </thead>
        <tbody id="list-order">
          <%= render partial: "order", collection: @orders %>
        </tbody>
      </table>
    </div>
  </div>
</div>
